dis=float(input('Digite a quantidade de kms percorridos pelo carro:'))
dias=int(input('Digite a quantidade de dias que o carro foi alugado:'))

km=dis*0.15
odia=dias*60
total=km+odia

print(f'O valor a pagar da quilometragem é de R${km:.2f}')
print(f'O valor do carro é de R${odia:.2f}') 
print(f'O valor total será de R${total:.2f}')
