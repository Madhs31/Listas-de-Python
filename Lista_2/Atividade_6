valor=float(input('Quanto você ganha por hora?'))
horas=int(input('Qual o número de horas trabalhadas?'))
bruto=valor*horas

des1=bruto*0.11
des2=bruto*0.08
des3=bruto*0.05
líquido=bruto-des1-des2-des3

print(f'Seu salário bruto é de {bruto:.2f}')
print(f'Seu IR é de R${des1:.2f}')
print(f'Seu INSS é de R${des2:.2f}')
print(f'O Sindicato é de R${des3:.2f}')
print(f'Seu salário líquido é de R${líquido:.2f}')
