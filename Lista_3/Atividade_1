nota=float(input('Digite uma nota de 0 a 10:'))
while (nota<0) or (nota>10):
     print ('Notas entre 0 e 10')
     nota = float(input('Digite uma nota: '))

#atividade 2

nome=input('Digite um nome de usuário:')
senha=input('Digite uma senha:')
while (nome==senha):
    print('Erro')
    nome=input('Digite um nome de usuário:')
    senha=input('Digite uma senha:')
